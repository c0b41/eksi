{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": "last 2 versions"
        }
      }
    ]
  ],
  "plugins": [
    "add-module-exports",
    [
		"@babel/plugin-transform-runtime",
		{
			"corejs": false,
			"helpers": false,
			"regenerator": true,
			"useESModules": false
		}
	]
  ]
}
